{
  "name": "branis/utmdb-client",
  "type": "project",
  "description": "An upcoming movies platform.",
  "keywords": [
    "movies",
    "upcoming",
    "branis"
  ],
  "license": "MIT",
  "require": {
    "ext-curl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "illuminate/database": "^6.0",
    "illuminate/pagination": "^6.0",
    "mnapoli/silly-php-di": "^1.2",
    "php-di/php-di": "^6.0",
    "slim/php-view": "^2.2",
    "slim/psr7": "^0.5.0",
    "slim/slim": "^4.2",
    "vlucas/phpdotenv": "^3.5"
  },
  "config": {
    "optimize-autoloader": true,
    "php": ">=7.3",
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 0
  },
  "autoload": {
    "psr-4": {
      "UpcomingMovies\\": "src/",
      "Foundation\\": "foundation/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": "phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  }
}
